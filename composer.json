{
  "name": "tourze/gitee-api-bundle",
  "description": "Gitee",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/orm": "^3.0",
    "symfony/framework-bundle": "^6.4",
    "symfony/console": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/easy-admin-attribute": "0.1.*",
    "symfony/dependency-injection": "^6.4",
    "symfony/config": "^6.4",
    "doctrine/dbal": "^4.0",
    "symfony/http-kernel": "^6.4",
    "doctrine/doctrine-bundle": "^2.13"
  },
  "autoload": {
    "psr-4": {
      "GiteeApiBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GiteeApiBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  }
}